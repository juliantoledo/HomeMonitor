<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>home-monitor-arduino</application>
    <module>api</module>
    <version>1</version>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
        <property name="DEFAULT_ENCODING" value="UTF-8" />
    </system-properties>

    <env-variables>
    	<!-- More info: https://cloud.google.com/endpoints/docs/frameworks/java/quickstart-frameworks-java -->
		<env-var name="ENDPOINTS_SERVICE_NAME" value="api-dot-home-monitor-arduino.appspot.com" />
	</env-variables>
</appengine-web-app>
